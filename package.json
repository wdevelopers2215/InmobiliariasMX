{
  "name": "InmobiliariasMX",
  "version": "2.0.5",
  "description": "Aplicaci√≥n para Inmobiliarios",
  "main": "main.js",
  "author": "W Developers",
  "scripts": {
    "start": "electron .",
    "win": "electron-builder -w",
    "mac": "electron-builder -m",
    "linux": "electron-builder -l",
    "build": "electron-builder -mwl",
    "make-cert": "electron-builder create-self-signed-cert -p certificado",
    "publish": "electron-builder build -w -p onTagOrDraft"
  },
  "build": {
    "appId": "com.wdevelopers.mx",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "publish": [
        "github"
      ],
      "certificateFile": "private/certificado.pfx",
      "verifyUpdateCodeSignature": false,
      "publisherName": "certificado"
    },
    "mac": {
      "publish": [
        "github"
      ]
    },
    "linux": {
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "electron-updater": "^4.0.6",
    "html2canvas": "^1.0.0-alpha.12",
    "jquery": "^3.4.0",
    "jspdf": "^1.5.3",
    "sweetalert2": "^7.31.1"
  },
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^20.40.2"
  }
}
